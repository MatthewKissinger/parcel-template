  "bugs": {
    "url": "https://github.com/MatthewKissinger/parcel-template/issues"
  },
  "homepage": "https://github.com/MatthewKissinger/parcel-template#readme",
  "devDependencies": {
    "@parcel/transformer-sass": "^2.5.0",
    "eslint": "^8.16.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.26.0",
    "parcel": "^2.5.0",
    "sass": "^1.52.1"
  }
}
