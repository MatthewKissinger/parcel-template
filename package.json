{
  "name": "parcel-template",
  "version": "1.0.0",
  "description": "Project starter template using parcel bundler",
  "source": "src/index.html",
  "scripts": {
    "start": "npx parcel src/index.html -p 3000 --open",
    "clean": "rm -rf dist/*",
    "build:parcel": "npx parcel build src/index.html",
    "build": "npm run clean && npm run build:parcel"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MatthewKissinger/parcel-template.git"
  },
  "keywords": [],
